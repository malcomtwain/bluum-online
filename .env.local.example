# Copiez ce fichier en .env.local et remplissez les valeurs

# Supabase (requis)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_KEY=your_supabase_service_key

# TikTok OAuth
# Client key est public, visible dans le code
NEXT_PUBLIC_TIKTOK_CLIENT_KEY=your_tiktok_client_key
# Client secret - gardez secret !
TIKTOK_CLIENT_SECRET=your_tiktok_client_secret

# Instagram OAuth (optionnel pour l'instant)
NEXT_PUBLIC_INSTAGRAM_CLIENT_ID=
INSTAGRAM_CLIENT_SECRET=

# Pour dev local (optionnel)
# Si vous utilisez ngrok ou autre tunnel
# NEXT_PUBLIC_NGROK_URL=https://xxx.ngrok.io

# PostFast API
POSTFAST_API_KEY=your_postfast_api_key